{
    "name": "Al-Fathiha",
    "label": "வெற்றிக்கு மேல் வெற்றி",
    "verses":[
        { 
            "order": [2, 3, 1, 0],
            "verse": [
                ["பெயரைக்கொண்டு.", "திருப்பெயரால்."],
                ["இறைவனின்", "அல்லாஹ்வின்"],
                ["அளவற்ற அருளாளன்"],
                ["நிகரற்ற அன்புடையோன்"]
            ]
        },
        { 
            "order": [0, 3, 2, 1],
            "verse": [
                ["எல்லா புகழும்", "அனைத்து புகழும்"],
                ["இறைவனுக்கே.", "அல்லாஹுக்கே."],
                ["அதிபதி", "எஜமானன்"],
                ["அகிலங்களின்", "பிரபஞ்சத்தின்"]
            ]
        },
        { 
            "order": [],
            "verse": [
                ["அளவற்ற அருளாளன்", "மிக்க அருளாளன்"],
                ["நிகரற்ற அன்புடையோன்.", "மிக்க அன்புடையோன்."]
            ]
        },
        { 
            "order": [2, 1, 0],
            "verse": [
                ["அதிபதி.", "அரசன்."],
                ["நாளின்"],
                ["நியாயத் தீர்ப்பு", "ஒப்பந்த", "கடன் தீர்ப்பு"]
            ]
        },
        { 
            "order": [],
            "verse": [
                ["உன்னிடமே"],
                ["நாங்கள் வணங்குகிறோம்,"],
                ["மற்றும் உன்னிடமே"],
                ["நாங்கள் உதவியும் தேடுகிறோம்."]
            ]
        },
        {
            "order": [2, 1, 0],
            "verse": [
                ["நடத்துவாய்."],
                ["வழியில்"],
                ["நேர்", "நிறுவப்பட்ட நேர்"]
            ]
        },
        {
            "order": [3, 2, 1, 0, 5, 6, 4, 7, 8],
            "verse": [
                ["வழி,"],
                ["அவர்களுடய"],
                ["நீ அருள்புரிந்தாயோ"],
                ["எவர்களுக்கு"],
                ["அல்ல,"],
                ["உன் கோபத்திற்கு"],
                ["ஆளானோர்"],
                ["மற்றும் அல்ல"],
                ["வழிதவறியவர்கள்."]
            ]
        }
    ]
}